try:
    while True:
        listLen = int(input())
        list1 = []
        myDict = {}
        for i in range(listLen):
            list1.append(int(input()))
        list1Sorted = sorted(list1)
        list2 = []
        for i in range(listLen):
            list2.append(int(input()))
        list2 = sorted(list2)
        for i in range(listLen):
            myDict[list1Sorted[i]] = list2[i]
        for i in list1:
            print(myDict[i])
        print("")
except:
    pass